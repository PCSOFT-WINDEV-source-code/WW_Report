     name : InitializeGraph
     procedure_id : 2217187621471109310
     type_code : 14
     code : |1-
      procedure InitializeGraph()
      
      	nIndex 	is int = 1
      	sCondition	is string = ""
      
      // Specify a spacing between the categories of the bar chart
      grColumnSpacing(SalesGraph, 10)
      
      // Modify the direction of the chart
      grOrientation(SalesGraph, grYCoordinate, grAtRight, grAtTop)
      
      // Define the mark
      grGraduate(SalesGraph, 7000, grYCoordinate)
      
      // Read the first record
      HReadFirst(QRY_CATEGORY)
      
      while not HOut()
      	// Add data to the chart
      	grAddData(SalesGraph, 1, nIndex,QRY_CATEGORY.TotalIOT)
      
      	grCategoryLabel(SalesGraph, nIndex, QRY_CATEGORY.ProdDesc + <§$0000§> + QRY_CATEGORY.OrderLineID)
      	grLabel(SalesGraph, grShowValue, True)
      	
      	// Read the next record
      	HReadNext(QRY_CATEGORY)
      
      	// Increment the browse subscript
      	nIndex++
      end
